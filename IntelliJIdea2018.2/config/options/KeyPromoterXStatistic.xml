<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B' or 'Ctrl+Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>8</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete...">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'Delete'</ShortCut>
          <Description>Delete...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find Usages">
        <Item>
          <IdeaActionID>FindUsages</IdeaActionID>
          <ShortCut>'Alt+F7'</ShortCut>
          <Description>Find Usages</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="New Folder...">
        <Item>
          <IdeaActionID>FileChooser.NewFolder</IdeaActionID>
          <ShortCut>'Alt+Insert' or 'Ctrl+N'</ShortCut>
          <Description>New Folder...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'VIIDApplication'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Shift+F10'</ShortCut>
          <Description>Run 'VIIDApplication'</Description>
          <Count>7</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'VIIDApplication'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Stop 'VIIDApplication'</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>